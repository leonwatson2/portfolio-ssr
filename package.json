{
  "name": "portfolio-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/npm-run-all -p webpack:w server-dev sass:w",
    "webpack": "node_modules/.bin/webpack --mode=deployment ",
    "webpack:w": "node_modules/.bin/webpack --mode=development -w",
    "server-dev": "node_modules/.bin/nodemon server.js",
    "server": "node server.js",
    "sass": "node_modules/.bin/node-sass --output-style compressed src/style.sass public/style.css",
    "sass:w": "node_modules/.bin/node-sass --output-style compressed -rw src/style.sass public/style.css",
    "build": "node_modules/.bin/npm-run-all -p webpack server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "description": "Leon's Portfolio",
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "serialize-javascript": "^1.6.1"
  }
}
